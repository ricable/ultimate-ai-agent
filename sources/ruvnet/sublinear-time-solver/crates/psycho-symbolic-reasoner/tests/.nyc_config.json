{
  "extends": "@istanbuljs/nyc-config-typescript",
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/test-utils/**",
    "src/setup/**",
    "dist/**",
    "node_modules/**",
    "coverage/**"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov",
    "json"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./coverage/.nyc_output",
  "check-coverage": true,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "per-file": true,
  "all": true,
  "skip-full": false,
  "cache": true,
  "produce-source-map": true,
  "instrument": true,
  "source-map": true
}