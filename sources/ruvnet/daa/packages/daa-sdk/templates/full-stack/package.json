{
  "name": "daa-fullstack-agent",
  "version": "1.0.0",
  "description": "Full-stack DAA agent with orchestrator, QuDAG, and Prime ML",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "node --test",
    "test:orchestrator": "ts-node src/orchestrator.ts",
    "test:qudag": "ts-node src/qudag.ts",
    "test:workflows": "ts-node src/workflows.ts",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "daa",
    "orchestrator",
    "qudag",
    "mrap",
    "workflow",
    "autonomous-agents"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "daa-sdk": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
