{
  "name": "neuro-federated-ran-optimizer",
  "version": "1.0.0",
  "description": "Neuro-Federated Swarm Intelligence for Ericsson RAN Optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "init-swarm": "npx claude-flow@alpha init --force --sublinear --neural",
    "spawn-optimizer": "npx agentic-flow --agent optimizer --optimize",
    "start-consciousness": "npx strange-loops-mcp start"
  },
  "keywords": [
    "ran",
    "5g",
    "ericsson",
    "swarm-intelligence",
    "federated-learning",
    "neural-network",
    "telecommunications"
  ],
  "author": "RAN Optimization Team",
  "license": "MIT",
  "dependencies": {
    "@ruvnet/bmssp": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
