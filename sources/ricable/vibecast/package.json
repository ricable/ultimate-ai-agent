{
  "name": "ericsson-ran-timeseries-platform",
  "version": "1.0.0",
  "description": "Advanced time series analysis and prediction platform for Ericsson RAN automation with multi-agent orchestration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cargo build --release --manifest-path=rust-engine/Cargo.toml",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "orchestrator": "tsx src/orchestrator/index.ts",
    "agent:ran-docs": "tsx src/agents/ran-docs-agent.ts",
    "agent:alarms": "tsx src/agents/alarm-fault-agent.ts",
    "agent:kpi": "tsx src/agents/kpi-analyzer-agent.ts",
    "agent:config": "tsx src/agents/config-management-agent.ts"
  },
  "keywords": [
    "ericsson",
    "ran",
    "timeseries",
    "prediction",
    "automation",
    "multi-agent",
    "claude-ai"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "agentic": "^9.1.1",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1",
    "dotenv": "^16.4.7",
    "winston": "^3.17.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/lodash-es": "^4.17.12",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
