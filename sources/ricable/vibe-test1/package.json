{
  "name": "ericsson-ran-swarm-optimizer",
  "version": "1.0.0",
  "description": "Autonomous Federated Swarm Architecture for Ericsson RAN Optimization",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:wasm",
    "build:wasm": "wasm-pack build --target web ./rust-core",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "simulate": "tsx src/simulation/swarm-simulator.ts",
    "dashboard": "concurrently \"npm run dashboard:backend\" \"npm run dashboard:frontend\"",
    "dashboard:backend": "tsx src/api/server.ts",
    "dashboard:frontend": "cd dashboard/frontend && npm run dev",
    "train:rl": "tsx src/ran/cm/train-rl-agent.ts",
    "swarm:deploy": "tsx scripts/deploy-swarm.ts"
  },
  "dependencies": {
    "@ruvector/attention-wasm": "^0.1.0",
    "@ruvector/cluster": "^0.1.0",
    "@ruvector/gnn-wasm": "^0.1.0",
    "@ruvector/graph-wasm": "^0.1.0",
    "@ruvector/postgres-cli": "^0.2.0",
    "@ruvector/router-wasm": "^0.1.0",
    "@ruvector/ruvllm-darwin-arm64": "^0.2.0",
    "@ruvector/server": "^0.1.0",
    "@ruvector/sona-darwin-arm64": "^0.1.4",
    "@ruvector/tiny-dancer-wasm": "^0.1.0",
    "@ruvector/wasm": "^0.1.16",
    "@tensorflow/tfjs-node": "^4.15.0",
    "better-sqlite3": "^9.2.2",
    "date-fns": "^3.0.6",
    "decimal.js": "^10.4.3",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "fft-js": "^0.0.12",
    "hnswlib-node": "^2.1.0",
    "lodash-es": "^4.17.21",
    "mathjs": "^12.2.1",
    "ml-matrix": "^6.10.5",
    "nanoid": "^5.0.4",
    "prom-client": "^15.1.0",
    "protobufjs": "^7.2.5",
    "rxjs": "^7.8.1",
    "simple-statistics": "^7.8.3",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "wasm-pack": "^0.12.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ericsson",
    "ran",
    "5g",
    "optimization",
    "federated-learning",
    "swarm-intelligence",
    "gnn",
    "reinforcement-learning",
    "self-organizing-network"
  ],
  "author": "RAN Optimization Team",
  "license": "MIT"
}
