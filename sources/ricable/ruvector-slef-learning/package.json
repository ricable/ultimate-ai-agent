{
  "name": "edge-native-ai-saas",
  "version": "1.0.0",
  "description": "Decentralized Edge-Native AI Architecture: A Comprehensive Framework for Distributed SaaS using Kairos, SpinKube, Talos, Omni, and Agentic Protocols",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npx claude-flow@alpha init",
    "setup:mac": "bash scripts/setup-mac-silicon.sh",
    "dev": "docker compose up -d",
    "dev:gpu": "docker compose --profile gpu up -d",
    "dev:observability": "docker compose --profile observability up -d",
    "stop": "docker compose down",
    "logs": "docker compose logs -f",
    "ruvector": "npx ruvector",
    "ruvector:gnn": "npx ruvector-gnn",
    "ruvector:graph": "npx ruvector-graph",
    "ruvector:postgres": "npx @ruvector/postgres-cli",
    "agentdb": "npx agentdb@alpha",
    "claude-flow": "npx claude-flow@alpha",
    "agentic-flow": "npx agentic-flow@alpha",
    "mcp:start": "npx agentic-flow@alpha mcp start",
    "mcp:status": "npx agentic-flow@alpha mcp status",
    "agents:list": "npx agentic-flow@alpha --list",
    "wasm:build": "bash scripts/wasm/build-wasm-packages.sh",
    "wasm:build:push": "bash scripts/wasm/build-wasm-packages.sh --push",
    "wasm:deploy": "bash scripts/wasm/deploy-wasm-runtime.sh deploy",
    "wasm:verify": "bash scripts/wasm/deploy-wasm-runtime.sh verify",
    "wasm:cleanup": "bash scripts/wasm/deploy-wasm-runtime.sh cleanup",
    "wasmedge:run": "npx wasmedge-quickjs",
    "llamaedge:run": "bash scripts/llamaedge/run-llamaedge.sh",
    "talos:apply": "bash scripts/talos/talos-apply.sh",
    "talos:status": "talosctl cluster show",
    "omni:sync": "omnictl cluster template sync -v -f infrastructure/talos/cluster-template.yaml",
    "omni:status": "omnictl get clusters",
    "k8s:apply": "kubectl apply -k infrastructure/",
    "k8s:delete": "kubectl delete -k infrastructure/",
    "test": "bash tests/wasm/test-wasm-agents.sh",
    "test:wasm": "bash tests/wasm/test-wasm-agents.sh",
    "test:ruvector": "npx ruvector test",
    "test:agentdb": "npx agentdb@alpha test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ricable/ruvector-slef-learning"
  },
  "keywords": [
    "edge-native",
    "ai",
    "saas",
    "kairos",
    "spinkube",
    "webassembly",
    "wasmedge",
    "llamaedge",
    "spin",
    "wasm",
    "k3s",
    "kubernetes",
    "talos",
    "omni",
    "siderolabs",
    "distributed",
    "agentic",
    "mcp",
    "a2a",
    "vector-database",
    "ruvector",
    "ruvector-gnn",
    "ruvector-graph",
    "claude-flow",
    "agentic-flow",
    "agentdb",
    "runtime-class-manager"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ruvector/agentic-synth-examples": "^0.1.6",
    "@ruvector/gnn": "^0.1.22",
    "@ruvector/graph-node": "^0.1.15",
    "@ruvector/postgres-cli": "latest",
    "agentdb": "^1.6.1",
    "agentic-flow": "^1.10.2",
    "claude-flow": "^2.7.41",
    "ruvector": "^0.1.31",
    "ruvector-extensions": "^0.1.0",
    "ruvector-gnn": "latest",
    "ruvector-graph": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
