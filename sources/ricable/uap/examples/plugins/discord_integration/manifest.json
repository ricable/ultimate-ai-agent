{
  "plugin_id": "discord-integration-example",
  "name": "discord_integration",
  "display_name": "Discord Integration",
  "version": "1.0.0",
  "description": "Send messages and manage Discord servers through UAP",
  "author": "UAP Team",
  "author_email": "plugins@uap.ai",
  "homepage": "https://github.com/uap/plugins/discord",
  "repository": "https://github.com/uap/plugins/discord",
  "license": "MIT",
  "plugin_type": "integration",
  "category": "communication",
  "tags": ["discord", "chat", "gaming", "integration"],
  "python_version": ">=3.11",
  "dependencies": ["aiohttp>=3.8.0", "pydantic>=2.0.0"],
  "permissions": ["network_access", "store_credentials"],
  "main_module": "discord_integration",
  "main_class": "DiscordIntegration",
  "config_schema": {
    "type": "object",
    "properties": {
      "bot_token": {
        "type": "string",
        "description": "Discord bot token",
        "secret": true
      },
      "default_channel": {
        "type": "string",
        "description": "Default channel ID for messages",
        "default": ""
      },
      "command_prefix": {
        "type": "string",
        "description": "Prefix for bot commands",
        "default": "!"
      }
    },
    "required": ["bot_token"]
  },
  "default_config": {
    "command_prefix": "!",
    "default_channel": ""
  },
  "logo_url": "https://cdn.worldvectorlogo.com/logos/discord-6.svg",
