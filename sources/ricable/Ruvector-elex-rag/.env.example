# RuVector Telecom RAG Configuration
# Copy this to .env and fill in your values

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Embedding Configuration
EMBEDDING_MODEL=text-embedding-3-large
EMBEDDING_DIMENSIONS=3072

# Vector Database Settings
RUVECTOR_STORAGE_PATH=./data/vector_store
RUVECTOR_INDEX_TYPE=hnsw
RUVECTOR_M=32
RUVECTOR_EF_CONSTRUCTION=200
RUVECTOR_EF_SEARCH=100

# Graph Database Settings
GRAPH_STORAGE_PATH=./data/graph_store
GRAPH_MAX_NODES=1000000
GRAPH_MAX_EDGES=10000000

# GNN Configuration
GNN_HIDDEN_DIM=256
GNN_NUM_LAYERS=4
GNN_DROPOUT=0.1
GNN_LEARNING_RATE=0.001
GNN_BATCH_SIZE=64

# Bayesian Settings
BAYESIAN_MC_SAMPLES=100
BAYESIAN_PRIOR_SCALE=1.0
UNCERTAINTY_THRESHOLD=0.5

# Agent Swarm Configuration
SWARM_MAX_AGENTS=100
SWARM_COORDINATION_INTERVAL=5000
AGENT_EXPLORATION_RATE=0.1
AGENT_LEARNING_RATE=0.01

# Optimization Settings
OPTIMIZATION_POPULATION_SIZE=50
OPTIMIZATION_GENERATIONS=100
MUTATION_RATE=0.1
CROSSOVER_RATE=0.7

# Coverage & Capacity Thresholds
MIN_EDGE_SINR_DB=-5
MAX_INTERFERENCE_DB=-90
TARGET_SPECTRAL_EFFICIENCY=3.0

# 3GPP Parameter Ranges
ALPHA_MIN=0.4
ALPHA_MAX=1.0
P0_MIN=-126
P0_MAX=24

# Data Paths
ELEX_DATA_PATH=./data/elex
THREEGPP_DATA_PATH=./data/3gpp
PROCESSED_DATA_PATH=./data/processed

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080
ENABLE_SWAGGER=true

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Ericsson ENM Integration (optional)
ENM_API_URL=
ENM_USERNAME=
ENM_PASSWORD=
ENM_ENABLED=false
