# ====================
# ORCHESTRATION CONFIG
# ====================

# Primary LLM Provider (local, openrouter, anthropic, openai)
LLM_PROVIDER=local

# Local GaiaNet Node
GAIANET_ENDPOINT=http://localhost:8080/v1
GAIANET_MODEL=Qwen2.5-Coder-32B-Instruct

# Fallback Cloud Providers (optional)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
OPENROUTER_API_KEY=

# ====================
# AGENT CONFIGURATION
# ====================

# Agent Booster (WASM acceleration)
ENABLE_AGENT_BOOSTER=true

# ReasoningBank persistence
REASONING_BANK_PATH=./agent-db/reasoning-bank
AGENT_DB_PATH=./agent-db/vector-store

# Agent behavior
MAX_ITERATIONS=10
CONTEXT_WINDOW=32768
TEMPERATURE=0.2

# ====================
# SANDBOX CONFIGURATION
# ====================

# Sandbox type (docker, e2b)
SANDBOX_TYPE=docker

# Docker sandbox settings
DOCKER_SANDBOX_IMAGE=node:20-alpine
SANDBOX_MEMORY_LIMIT=2g
SANDBOX_CPU_LIMIT=2
SANDBOX_NETWORK=isolated
SANDBOX_TIMEOUT=300000

# E2B settings (if using cloud sandbox)
E2B_API_KEY=

# ====================
# WASMEDGE / LLAMAEDGE
# ====================

# WasmEdge installation path
WASMEDGE_PATH=/usr/local/bin/wasmedge

# WASI-NN backend (mlx, ggml, openvino)
WASI_NN_BACKEND=mlx

# Model paths
LLAMAEDGE_MODEL_PATH=./models/qwen-2.5-coder-32b.gguf

# ====================
# GAIANET MONETIZATION
# ====================

# Node configuration
GAIANET_NODE_ID=
GAIANET_DOMAIN=developer-tools
GAIANET_PUBLIC_ENDPOINT=

# Staking (when available)
GAIA_WALLET_ADDRESS=
GAIA_STAKE_AMOUNT=

# ====================
# PERFORMANCE TUNING
# ====================

# WASM optimization
WASM_THREADS=8
WASM_MEMORY_PAGES=65536

# MLX GPU acceleration
MLX_ENABLE_GPU=true
MLX_MEMORY_FRACTION=0.8

# ====================
# DISTRIBUTED CLUSTER
# ====================

# Node identification
NODE_NAME=my-node
NODE_ID=

# Redis coordination
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Cluster mode (standalone, distributed)
CLUSTER_MODE=standalone

# ====================
# MONITORING
# ====================

# Enable telemetry
ENABLE_TELEMETRY=true
LOG_LEVEL=info
