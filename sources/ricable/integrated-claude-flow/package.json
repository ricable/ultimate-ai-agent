{
  "name": "integrated-claude-flow-commands",
  "version": "1.0.0",
  "description": "Integrated claude-flow command suite for AI-driven development workflows",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "node benchmark/run-performance-tests.js",
    "build": "npm run build:coordination && npm run build:memory && npm run build:commands",
    "build:coordination": "tsc -p coordination/tsconfig.json",
    "build:memory": "tsc -p memory/tsconfig.json", 
    "build:commands": "tsc -p commands/tsconfig.json",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "typecheck": "tsc --noEmit",
    "swarm:init": "npx claude-flow@alpha swarm init",
    "swarm:status": "npx claude-flow@alpha swarm status",
    "swarm:monitor": "npx claude-flow@alpha swarm monitor",
    "memory:backup": "npx claude-flow@alpha memory backup",
    "memory:restore": "npx claude-flow@alpha memory restore",
    "benchmark": "node benchmark/benchmark-runner.js",
    "dev": "nodemon index.js",
    "docs:build": "typedoc --out docs src",
    "clean": "rm -rf dist/ coverage/ .swarm/ coordination/memory_bank/"
  },
  "keywords": [
    "ai",
    "automation",
    "swarm",
    "orchestration",
    "claude-flow",
    "multi-agent",
    "coordination",
    "neural-networks",
    "performance",
    "github-integration"
  ],
  "author": "Claude-Flow Integration Team",
  "license": "MIT",
  "dependencies": {
    "claude-flow": "^2.0.0-alpha",
    "sqlite3": "^5.1.6",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "lodash": "^4.17.21",
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "@octokit/rest": "^20.0.2",
    "yaml": "^2.3.4",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.2",
    "eslint": "^8.54.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "typedoc": "^0.25.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cedric-dev/integrated-claude-flow.git"
  },
  "bugs": {
    "url": "https://github.com/cedric-dev/integrated-claude-flow/issues"
  },
  "homepage": "https://github.com/cedric-dev/integrated-claude-flow#readme"
}